msgstr "[Installeret,lokalt]"

#: apt-private/private-output.cc
msgid "[installed,auto-removable]"
msgstr "[installeret,kan auto-fjernes]"

#: apt-private/private-output.cc
msgid "[installed,automatic]"
msgstr "[Installeret,automatisk]"

#: apt-private/private-output.cc
msgid "[installed]"
